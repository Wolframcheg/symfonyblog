{% extends 'base.html.twig' %}

{% block body_all %}
<div class="container">
    <div class="login-box">
        <h1 class="login-logo">{% block title %}Login{% endblock %}</h1>
        {% if error %}
            <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
        {% endif %}
        {{ form(form) }}
    </div>
</div>
{% endblock %}
